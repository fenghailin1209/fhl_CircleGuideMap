    xmlns:fhl="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" 
    android:orientation="vertical"
    >
   <com.fhl.customwidget.view.GuideMapViewGroup
        fhl:PaintWidth="6"
        fhl:DashedSpace="15"
        fhl:PaintColor="@color/orange"
